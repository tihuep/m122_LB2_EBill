<XML-FSCM-INVOICE-2003A>
  <INTERCHANGE>
    <IC-SENDER>
      <Pid>{pid_sender}</Pid>
    </IC-SENDER>
    <IC-RECEIVER>
      <Pid>{pid_receiver}</Pid>
    </IC-RECEIVER>
    <IR-Ref />
  </INTERCHANGE>
  <INVOICE>
    <HEADER>
      <FUNCTION-FLAGS>
        <Confirmation-Flag />
        <Canellation-Flag />
      </FUNCTION-FLAGS>
      <MESSAGE-REFERENCE>
        <REFERENCE-DATE>
!          <Reference-No>202007164522001</Reference-No>
!          <Date>20200731</Date>
        </REFERENCE-DATE>
      </MESSAGE-REFERENCE>
      <PRINT-DATE>
        <Date>{print_date}</Date>
      </PRINT-DATE>
      <REFERENCE>
        <INVOICE-REFERENCE>
          <REFERENCE-DATE>
            <Reference-No>{invoice_nr}</Reference-No>
            <Date>{invoice_date}</Date>
          </REFERENCE-DATE>
        </INVOICE-REFERENCE>
        <ORDER>
          <REFERENCE-DATE>
            <Reference-No>order_nr</Reference-No>
            <Date>{order_date}</Date>
          </REFERENCE-DATE>
        </ORDER>
        <REMINDER Which="MAH">
          <REFERENCE-DATE>
            <Reference-No></Reference-No>
            <Date></Date>
          </REFERENCE-DATE>
        </REMINDER>
        <OTHER-REFERENCE Type="ADE">
          <REFERENCE-DATE>
            <Reference-No>12345678</Reference-No>
            <Date>{ade_date}</Date>
          </REFERENCE-DATE>
        </OTHER-REFERENCE>
      </REFERENCE>
      <BILLER>
        <Tax-No>{tax_no}</Tax-No>
        <Doc-Reference Type="ESR-ALT "></Doc-Reference>
        <PARTY-ID>
          <Pid>{pid_sender}</Pid>
        </PARTY-ID>
        <NAME-ADDRESS Format="COM">
          <NAME>
            <Line-35>{firstname} {lastname}</Line-35>
            <Line-35>{address}</Line-35>
            <Line-35>{zip} {city}</Line-35>
            <Line-35></Line-35>
            <Line-35></Line-35>
          </NAME>
          <STREET>
            <Line-35></Line-35>
            <Line-35></Line-35>
            <Line-35></Line-35>
          </STREET>
          <City></City>
          <State></State>
          <Zip></Zip>
          <Country></Country>
        </NAME-ADDRESS>
        <BANK-INFO>
          <Acct-No></Acct-No>
          <Acct-Name></Acct-Name>
          <BankId Type="BCNr-nat" Country="CH">001996</BankId>
        </BANK-INFO>
      </BILLER>
      <PAYER>
        <PARTY-ID>
          <Pid>{pid_receiver}</Pid>
        </PARTY-ID>
        <NAME-ADDRESS Format="COM">
          <NAME>
            <Line-35>{receiver_name}</Line-35>
            <Line-35>{receiver_address}</Line-35>
            <Line-35>{receiver_zip_and_city}</Line-35>
            <Line-35></Line-35>
            <Line-35></Line-35>
          </NAME>
          <STREET>
            <Line-35></Line-35>
            <Line-35></Line-35>
            <Line-35></Line-35>
          </STREET>
          <City></City>
          <State></State>
          <Zip></Zip>
          <Country></Country>
        </NAME-ADDRESS>
      </PAYER>
    </HEADER>
    <LINE-ITEM />
    <SUMMARY>
      <INVOICE-AMOUNT>
        <Amount>{invoice_sum}</Amount>
      </INVOICE-AMOUNT>
      <VAT-AMOUNT>
        <Amount></Amount>
      </VAT-AMOUNT>
      <DEPOSIT-AMOUNT>
        <Amount></Amount>
          <REFERENCE-DATE>
            <Reference-No></Reference-No>
            <Date></Date>
          </REFERENCE-DATE>
      </DEPOSIT-AMOUNT>
      <EXTENDED-AMOUNT Type="79">
        <Amount></Amount>
      </EXTENDED-AMOUNT>
      <TAX>
        <TAX-BASIS>
          <Amount></Amount>
        </TAX-BASIS>
        <Rate Categorie="S">0</Rate>
        <Amount></Amount>
      </TAX>
      <PAYMENT-TERMS>
        <BASIC Payment-Type="ESR" Terms-Type="1">
          <TERMS>
            <Payment-Period Type="M" On-Or-After="1" Reference-Day="{invoice_date_day}">{payment_limit_days}</Payment-Period>
            <Date>{payment_date_limit}</Date>
          </TERMS>
        </BASIC>
        <DISCOUNT Terms-Type="22">
          <Discount-Percentage>0.0</Discount-Percentage>
          <TERMS>
            <Payment-Period Type="M" On-Or-After="1" Reference-Day="{invoice_date_day}"></Payment-Period>
            <Date></Date>
          </TERMS>
          <Back-Pack-Container Encode="Base64"> </Back-Pack-Container>
        </DISCOUNT>
      </PAYMENT-TERMS>
    </SUMMARY>
  </INVOICE>
</XML-FSCM-INVOICE-2003A>
